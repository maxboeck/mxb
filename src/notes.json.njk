---
permalink: notes.json
excludeFromSitemap: true
---
[
{% for note in collections.notes %}
    {
        "id": {{ loop.revindex }},
        "date": "{{- note.date | iso -}}",
        "url": "{{- note.url | url | absoluteUrl(site.url) -}}",
        "content": "{{- note.templateContent | excerpt | striptags -}}",
        "syndicate": {{- note.data.syndicate | string -}}
    }
    {%- if not loop.last -%},{%- endif -%}
{% endfor %}
]