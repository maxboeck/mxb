        "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
        "@babel/core": "^7.11.4",
        "@babel/plugin-transform-react-jsx": "^7.12.12",
        "@babel/plugin-transform-runtime": "^7.11.0",
        "@babel/preset-env": "^7.11.0",
        "@netlify/functions": "^2.4.1",
        "babel-loader": "^8.2.2",
